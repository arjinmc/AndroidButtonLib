<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideButton">
        <attr name="tipsColor" format="color"/>
        <attr name="tipsTextSize" format="dimension"/>
        <attr name="button" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DownloadButton">
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="radius" format="integer"/>
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SubmitButton">
        <attr name="SubmitButton.themeColor" format="color"/>
        <attr name="SubmitButton.borderColor" format="color"/>
        <attr name="SubmitButton.errorColor" format="color"/>
        <attr name="SubmitButton.textSize" format="dimension"/>
        <attr name="SubmitButton.radius" format="integer"/>
        <attr name="SubmitButton.thickness" format="dimension"/>
    </declare-styleable>
</resources>